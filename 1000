<script async type="text/javascript" src="//jzas.faisys.com/js/dist/mallSvg.min.js?v=202009231236"></script>
<link type="text/css" href="//jzas.faisys.com/css/mallV2/mall_layout.min.css?v=202010141231" rel="stylesheet" />
<script defer type="text/javascript" src="//jzas.faisys.com/js/mallV2/mall_nav.min.js?v=202011191413"></script>
<script defer type="text/javascript" src="//jzas.faisys.com/js/mallV2/mall_layout.min.js?v=202011181242"></script>
<script defer type="text/javascript" src="//jzas.faisys.com/js/mallV2/mall_index.min.js?v=202011191026"></script>
<script defer type="text/javascript" src="//fe.faisys.com/material_library_1_0/js/vendor.min.js?v=202010141505"></script>
<script defer type="text/javascript" src="//fe.faisys.com/material_library_1_0/js/app.min.js?v=202010281702"></script>
<link defer type="text/css" href="//fe.faisys.com/material_library_1_0/css/app.min.css?v=202010291424" rel="stylesheet" />





	
	<script type="text/javascript">
    Vue.$eventBus = Vue.$eventBus || new Vue();

	(function(Vue, FUNC, undefined){
	
		$(init);
		//初始化页面
		function init(){
			//打开的方式
			//1、带特定参数访问
			//2、ab测试
			if(false){//带参数打开
				if(false){
					Site.showTopManageFrameFromJzallSite('', false);
				}
			}else if(false){//默认使用概况
				if(true){//商城B类
					Site.showTopManageFrameFromJzallSite('manageFrameSiteStatMall', false);//网站管理-概况
				}else{//官网
					if (false) {
						Site.showTopManageFrameFromJzallSite('manageFrameJZAllSiteStat', false);// 聚合概况页
					} else {
						Site.showTopManageFrameFromJzallSite('manageFrameSiteStatOverview');//网站管理-网站概况
					}
				}
			}
		
			FUNC.Storage.$on("onCurrentSiteNameUpdated", function(siteName){
				Vue.$eventBus.$emit("onCurrentSiteNameUpdated", siteName);//更新网站名称
			});
		}
	}(Vue, Site));

	//支持弹窗填写完成后的动画回调
	(function(Vue, FUNC, undefined){	
		//设计器入口的位置
		var designerEtrancePos = {
			x: 0,
			y: 0
		}
		//弹窗的位置
		var popupPos = {
			x: 0,
			y: 0
		};
	
		//获取创建弹窗运动结束的位置
		FUNC.getCreateSitePupupTransEndPos = function(){
			var designCenterPos = {
				x: designerEtrancePos.x+183,//在嵌入的场景下往右移
				y: designerEtrancePos.y
			};

			var popupCenterPos = getCreateSitePopupCenterPosition();

			var offset_left = popupCenterPos.x - designCenterPos.x;
			var offset_top = popupCenterPos.y - designCenterPos.y;

			return {
				x:offset_left,
				y: offset_top
			}
		};

		//更新弹窗位置
		FUNC.Storage.$on("onUpdatePopupPos", function(posJSON){
			var pos = JSON.parse(posJSON);
			if(pos.x!==0 && pos.y !== 0){
			    popupPos = pos;
			}
		});
		//更新设计器位置
		FUNC.Storage.$on("onUpdateDesignerEtrancePos", function(posJSON){
			var pos = JSON.parse(posJSON);
			if(pos.x!==0 && pos.y !== 0){
			    designerEtrancePos = pos;
			}
		});

		//刷新当前页面
		FUNC.Storage.$on("changeCurrUrl", function(url){
			if(Fai.isUrl(url)){
			    location.href = url;
			}
		})


		$(window).on("resize.recordDesignerEntrance", refreshHandler);

		Vue.nextTick(refreshHandler);

		function refreshHandler(){

			FUNC.Storage.$emit("onUpdateDesignerEtrancePos", JSON.stringify(getDesignerEntrancePos()));//获取设计器入口的坐标
			FUNC.Storage.$emit("onUpdatePopupPos", JSON.stringify(getCreateSitePopupCenterPosition()));//获取设计器入口的坐标
		}

		//获取设计器当前的位置
		function getDesignerEntrancePos(){
			var desingEntryWrap = document.querySelector('.J_designEntryWrap');//获取设计器容器节点
			if(!desingEntryWrap){
				return {
					x: 0,
					y: 0
				}
			}

			var containerScrollInfo = getContainerScrollInfo();

			var desingEntryWrapRect = desingEntryWrap.getBoundingClientRect();

			var width = desingEntryWrapRect.width;
			var height = desingEntryWrapRect.height;

			var center_x = desingEntryWrapRect.left + containerScrollInfo.scrollLeft + width/2;
			var center_y = desingEntryWrapRect.top + containerScrollInfo.scrollLeft + height/2;

			return {
				x: center_x,
				y: center_y
			}
		}

		//获取容器的滚动信息
		function getContainerScrollInfo(){
			var gMain = document.querySelector('#g_main');

			if(!gMain){
				return {
					scrollLeft : 0,
					scrollTop : 0
				}
			}

			var gMain_scrollTop = gMain.scrollTop;//获取滚动高度
			var gMain_scrollLeft = gMain.scrollLeft;//获取滚动高度

			return {
				scrollLeft: gMain_scrollLeft,
				scrollTop: gMain_scrollTop
			}
		}
		
		//获取创建弹窗的位置
		function getCreateSitePopupCenterPosition(){
			var popupDOM = document.querySelector('.J_createMallSiteInfo_wrap');
	
			if(!popupDOM){
				return {
				    x: 0,
				    y: 0
				}
			}

			var popupDOMRect = popupDOM.getBoundingClientRect();

			var width = popupDOMRect.width;
			var height = popupDOMRect.height;
			
			var containerScrollInfo = getContainerScrollInfo();

			return {
				x: popupDOMRect.left + containerScrollInfo.scrollLeft + popupDOMRect.width/2,
				y: popupDOMRect.top + containerScrollInfo.scrollTop + popupDOMRect.height/2
			}

		}
	}(Vue, Site));

	(function(Fai, Vue, FUNC, undefined){
	    
	    $(init);
		
		//刷新多网站聚合页和网站管理的url
	    FUNC.refreshAlllSiteUrlAndManageFrameUrl = function(allSiteUrl, manageFrameUrl){
			
			//这里的逻辑只会作为子窗口进行触发

			//把顶层窗口改成聚合页的链接，形成镜中镜	
			window.top.location.href = allSiteUrl;
			window.location.href = allSiteUrl;//当前触发者，刷新页面
	    }

	    //刷新tab
	    FUNC.refreshManageFrame = function(_openManageFrame){
	        _openManageFrame = _openManageFrame || '';
			
			//拿到网站管理的窗口，刷新页面
			if(window.parent.actions){
			    window.parent.actions.openPageWithMenuById(_openManageFrame);
			}
	    };
	    //切换url
	    FUNC.refreshManageFrameUrl = function(url, callback){
	        if(Fai.isUrl(url)){
	        	window.parent.location.href = url;
	        }
	    };

	    FUNC.sendMsgToManageFrame = function(cmdInfo, callback){
			callback = callback || function(){};

	        var cmdArr = entries(cmdInfo);
	        cmdArr.push(['v', Math.random()]);//加一个时间戳，防止iframe缓存不加载
	        var query = cmdArr.map(function(cmd){

	            //对url进行表面，避免特殊字符导致不加载
	            if(Fai.isUrl(cmd[1])){
	                cmd[1] = Fai.encodeUrl(cmd[1]);
	            }

	            return cmd.join("=");
	        }).join('&');

	        var $iframe = $('#manageFramePost');
	        var iframeUrl =  Fai.top.siteUrl+"/manageFramePost.jsp?"+query;
	        $iframe.attr("src", iframeUrl);

	        $iframe.off("load.callback")
	        	.on("load.callback", callback);

	    };

	    function init(){
	        //加载site的url
	        var $iframe = $('<iframe id="manageFramePost"></iframe>');
	        $iframe.css({
	            width: 1,
	            height: 1,
	            opacity: 0
	        });
	        $('body').append($iframe);
	    }

		//做兼容处理 by jser 
	    function entries(obj){
		    var ownProps = Object.keys( obj ),
		        i = ownProps.length,
		        resArray = new Array(i); // preallocate the Array
		    while (i--)
		      resArray[i] = [ownProps[i], obj[ownProps[i]]];
		    
		    return resArray;
	    }
	}(Fai, Vue, Site));

    $.cookie('_checkNewLvBrowser',true,{ expires: 30 });
    if(false) {
        $(function() {
            function cssTest() {
                var elem = document.createElement('x');  
                elem.style.cssText = 'pointer-events:auto';  
                return elem.style.pointerEvents === 'auto';  
            }
            if(cssTest()) {
                $('body').append("<svg id='GWWatermark' class='gw_watermark'><svg>")
            }
        })
    }
	</script>

	
