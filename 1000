	//处理中文域名
	function resovleChineseDomain(initDomain){
		if(initDomain.indexOf("https") != -1){
			var httpStr = initDomain.slice(0, 8);
        	initDomain = initDomain.slice(8);
		}else{
			var httpStr = initDomain.slice(0, 7);
        	initDomain = initDomain.slice(7);
		}
		var domainStrs = initDomain.split(".");
		for( var j = domainStrs.length - 1; j >= 0; j -- ){
			var tmp = domainStrs[j];
			if( tmp.indexOf( "xn--" ) === 0 ){
				//xn--开头 中文域名
				//取得punycode码
				tmp = tmp.slice( 4 );
				tmp = Fai.punycode.decode( tmp );
				
			}
			domainStrs[j] = tmp;
		}
		initDomain = domainStrs.join(".");
		initDomain = httpStr + initDomain;
		return initDomain;
	}

	var hideBuyMailTip = window.localStorage.getItem('hideBuyMailTip');
	if(hideBuyMailTip === 'true'){
		Fai.top.buyMailSuffix = '';
	}else{
		Fai.top.buyMailSuffix = "";
	}

	//创建多个网站的列表
	var companyNameData = [];
	for(var i = 0; i < Fai.top.siteLists.length; i++){
		//为了判断是否从O过来的 ，不然链接会出错
		href = Fai.top.currentUrl + '?siteId=' + Fai.top.siteLists[i].siteid;
		var company = {};
		company.type = 'serviceOnline';
		company.innerHtml = Fai.decodeHtml(Fai.top.siteLists[i].companyName);
		company.extClass = 'company';
		company.siteId = Fai.top.siteLists[i].siteid;
		company.del = Fai.top.siteLists[i].del;
		company.reason = Fai.top.siteLists[i].reason;
		company.clickEvent = 'changeSite';
		company.href = href;
		company.mainUrlBySiteId = resovleChineseDomain(Fai.top.siteLists[i].mainUrlBySiteId);
		company.verBySiteId = Fai.top.siteLists[i].verBySiteId;
		company.siteVersionBySiteId = Fai.top.siteLists[i].siteVersionBySiteId;
		company.vipExpireTimeValBySiteId = Fai.top.siteLists[i].vipExpireTimeValBySiteId;
		company.isOem = Fai.top.siteLists[i]._oem;
		company.headerUrl = Fai.top.siteLists[i].headerUrl;
		company.siteType = Fai.top.siteLists[i].siteType;
		company.isResponsive = Fai.top.siteLists[i].isResponsive;
		company.siteAdmHost = Fai.top.siteLists[i].siteAdmHost;



		companyNameData.push(company);	
	}
	
	
	Fai.top.companyNameData = companyNameData;


	
		Fai.top.jzHomeUrl = 'mall.fkw.com';
	


	
</script>

<script type="text/javascript">
	(function(Vue, FUNC, undefined){
	Vue.nextTick(function(){
		FUNC.Storage.init();
	})
	
	//创建storage，解决页面通信问题
	FUNC.Storage = {
		//初始化
		init: function(){
			var $self = this;
			var $emitHandler = function(event){
			    $self.$eventBus.$emit(event.key, $self.parseCacheValue(event.newValue));
			};
			//这货要处理兼容性问题。。。
			if( document.attachEvent && !$.browser.opera ) {       
			    document.attachEvent("onstorage", $emitHandler);       
			}else{       
				window.addEventListener("storage", $emitHandler, false);
			} 
		},
		$eventBus: new Vue(),
		$on: function(name, handler){
		    this.$eventBus.$on(name, handler)
		},
		$once: function(name, handler){
		    this.$eventBus.$once(name, handler)
		},
		$off: function(name){
			this.$eventBus.$off(name, handler);
		},
		$emit: function(name, value){
			//通过对value的设置，就能触发对应的注册事件
			localStorage.setItem(name, this.genCacheValue(value));
		},
		genCacheValue: function(value){
			var cacheValue = [value, Math.random()].join("@@@");
			return cacheValue;
		},
		parseCacheValue: function(value){
			if (!value) return;
			value = value.split("@@@")[0];
			return value;
		}
	};

}(Vue, Site));
</script>

