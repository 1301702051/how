									<div class="cloneMallTitle">
										请选择其他模板应用的站点：
									</div>
									<div class="msItem">
										

										
									</div>
							</div>
							<div id="goToCloneWeb" class="msCreateMallDisBtn">立即复制</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>


	<script type="text/javascript" src="//jzas.faisys.com/js/comm/SWFUpload2v/swfupload.min.js?v=201806151531"></script>

	<script>
	window.MallOpen = {};
	(function(MallOpen, Site){
		// 开通第一个网站
		var siteType = 2;
		var disCreateFirstSite = false;
		var hasMyWebForClone = false;
		var existCompanyGoal = true;
		(function(MallOpen, Site){
			$(".J_popOpenMall").css({"display":"flex"});
			setTimeout(function(){
			$("#openMallPopLoading").remove();
			}, 500);
			if(!disCreateFirstSite && !hasMyWebForClone){
				$(".createMallSite").hide();
			}

			

			// 开通网站
			MallOpen.openMallAjax = function(){
					$.ajax({
						type : 'post',
						url : '../ajax/site_h.jsp?cmd=setWafCk_openMall',
						error : function(){
							MallOpen.ing("服务繁忙，请稍后再试。");
						},
						success : function(data){
							var result = jQuery.parseJSON(data);
							if(result.success){
								//MallOpen.ing("开通商城成功，请创建一个站点。");
								$(".J_popOpenMall").hide();
								//mobi开通引导埋点
								if($.cookie('regFromMobi') == 'true'){
									Site.allSiteLogDog(200517, 36);
								}
								//site开通引导
								if($.cookie('regFromSite') == 'true'){
									Site.allSiteLogDog(200178, 42);
								}
								//轻站开通引导埋点
								if($.cookie('regFromWxapp') == 'true'){
									Site.allSiteLogDog(7000013, 20);
								}
								//互动引流开通引导埋点
								if(Fai.top.bizSource === 1){
									Site.allSiteLogDog(1000429, 5);
								}
							}else{
								MallOpen.ing(result.msg);
							}
						}
					});
			};

			var selectMallTypeValue = 0;
			MallOpen.checkCanCreateMall = function(){
				var mallName = $("#mallNameInput").val() || "";
				if($.trim(mallName) == ""){
					$("#msCreateMallBtn").removeClass("msCanCreateMallBtn");
					return;
				}

				if($(".mallTypeChooice.checkedType").length == 0){
					$("#msCreateMallBtn").removeClass("msCanCreateMallBtn");
					return;
				}else{
					selectMallTypeValue = parseInt($(".mallTypeChooice.checkedType").attr("type"));
				}

			$("#msCreateMallBtn").addClass("msCanCreateMallBtn");
			};
			//b类直销
			MallOpen.checkCreateMallButton = function(selectMallTypeValue){
				var mallName = $("#mallNameInput").val() || "";
				if($.trim(mallName) == "" || selectMallTypeValue === 0){
					$(".msCreateMallAppletsBtn").removeAttr("style");
					$(".computerImg").removeAttr("style");
					$(".smallImg").removeAttr("style");
					$(".msCreateMallAppletsBtn").removeClass("msCreateMallAppletsBtnHover");
					$(".msCreateMallComputerBtn").removeClass("msCreateMallComputerBtnHover");
					$(".msCreateMallSmallBtn").removeClass("msCreateMallSmallBtnHover");
					$(".msCreateMallAppletShape").removeClass("anim");
					return;
				}
				$(".msCreateMallAppletsBtn").css("opacity", "1");
				$(".computerImg").css("background-position", "-44px -441px");
				$(".smallImg").css("background-position", "-44px -482px");
				$(".msCreateMallAppletsBtn").addClass("msCreateMallAppletsBtnHover");
				$(".msCreateMallComputerBtn").addClass("msCreateMallComputerBtnHover");
				$(".msCreateMallSmallBtn").addClass("msCreateMallSmallBtnHover");
				$(".msCreateMallAppletShape").addClass("anim");
			};

			var cloneDemoId = 0;
			var cloneTypes = -1;
			var createCloneAid = -1;
			var cloneTemplateId = -1;
			var cloneDesignerType = 'site';
			var demoHostName = "";
			var fkHomeReg = false;
			var createTing = false;
			var reWriteUrlParam = cloneDemoId > 0 ? "?_createWeb=true" : "?_creatNewSite=true";
			var jumptoSiteHost = "//i.mall.fkw.com";
			var jumptoAllMall = "//i.mall.fkw.com";
			var jumptoAllMallOem = "//i.mall.vip.webportal.top";
			var mallAppUrl = "https://vb24076366.mallapp.fkw.com";
			
			Fai.top.isRegFromJz = false;
			Fai.top.isRegFromFkw = false;
			var jumptoMobiHost = "//vb24076366.jzm.fkw.com" + "/manage/mobiDesign.jsp";

			MallOpen.createMallSite = function(buttonType){
				// 防止不断的点击，不断的创建网站
				if(createTing){
					return;
				}

				var formData = MallOpen.mallOpenApp.getFormData();

				var siteType = 2;

				createTing = true;
				var result = {};
				var cloneSiteId = 0;
				var param = {
					'mallName':formData.mallName,
					"siteId": Fai.top.siteId
				};
				
				if(disCreateFirstSite){
					param.firstMallSite = true;
					param.companyGoal = formData.mallType;
				}

				// 先创建网站
				$.ajax({
					type : 'post',
					async: false, //同步请求
					data:param,
					url : '/ajax/site_h.jsp?cmd=setWafCk_createMallSite',
					error : function(){
					MallOpen.ing("服务繁忙，请稍后再试。");
					},
					success : function(data){
						result = jQuery.parseJSON(data);
						cloneSiteId = result.newSiteId;
						if(result.success){//要成功才能往下执行
							if(MallOpen.mallOpenApp.isCreateMallSiteSecondly){//二次创建站点
								var documentObj = document;
								if(Fai.isSafari()){
									documentObj = top.window.document;
								}
								//首次和二次创建站点，都需要标识_creatNewSite，点击跳转商城入口可出现极速建站弹窗 allen 2020-11-05
								documentObj.location.href = "//"+Fai.top._allSiteDomain+"?_creatNewSite=true&siteId="+cloneSiteId;
							}else if(!disCreateFirstSite){
								reWriteUrlParam = "?_redirectUrl="+Fai.encodeUrl(result.siteUrl);
								jumptoSiteHost = result.siteUrl;
								jumptoMobiHost = result.mobiUrl + "/manage/mobiDesign.jsp";
							}else{ //首次创建
								if(Fai.top._oem){  //分销
									jumptoSiteHost = jumptoAllMallOem;
								}else{
									if (buttonType === 1){//小程序商城
										Site.allSiteLogDog(200933, 7); //b类站点创建成功
										jumptoSiteHost = "https://vb24076366.mallapp.fkw.com";
									}else if (buttonType === 2){//电脑商城
										Site.allSiteLogDog(200933, 8);
									}else if (buttonType === 3) {  //微商城
										jumptoSiteHost = jumptoMobiHost;
										Site.allSiteLogDog(200933, 9);
									}else {      //a类创建成功
										jumptoSiteHost = jumptoAllMall;
										Site.allSiteLogDog(200933, 2);
									}
								}
							}
						}
						else {
							!!result.msg && MallOpen.ing(result.msg, false);
						}
					}
				});


				if(!result.success){
					MallOpen.ing(result.msg || result.message);
					createTing = false;
					return false;
				}
				if(disCreateFirstSite){
					Site.allSiteLogDog(200531, 1);
				}

				Site.allSiteLogDog(200645, 2);

				if(formData.mallType != 0){
					Site.allSiteLogDog(200647, formData.mallType);
				}

				history.pushState({}, "", reWriteUrlParam);

				//正常所有设计器的复制模板，应该在设计器内部
				if(result.success){
					Site.allSiteLogDog(200531, 1);//站点创建成功
					Site.allSiteLogDog(200985, 1);//站点创建成功-聚合页改版
			
					if(!MallOpen.mallOpenApp.isCreateMallSiteSecondly){
						var regAutoCloneUrl = MallOpen.getRegAutoCloneUrl();
						var isRegCloneJump = regAutoCloneUrl && Fai.isUrl(regAutoCloneUrl) && disCreateFirstSite;//cookie带特定参数，且没有创建过第一个站点，走复制逻辑 by jser 2019-5-9
						if(isRegCloneJump){//注册前有尝试克隆过模板的
							document.location.href = regAutoCloneUrl;
						}else if(false){//带克隆参数进入开通界面
							document.location.href =  "//vb24076366.mall.fkw.com/?clone=0&cloneDesignerType=site&_creatNewSite=true"
						}

						//url带特定参数，走复制逻辑
						//if(false){
							//document.location.href =  "//vb24076366.mall.fkw.com/?clone=0&cloneDesignerType=site&_creatNewSite=true";
						//}else if(isRegCloneJump){
						//	document.location.href = regAutoCloneUrl;
						//}else{
						//   if (buttonType === 3){
						//        document.location.href = jumptoSiteHost + "?_openMobiRapid=true&_creatNewSite=true";
						//    }else {
						//        document.location.href = jumptoSiteHost + "?_creatNewSite=true";
						//    }
						//}

					}
				}else{
					MallOpen.ing(result.msg);
				}

				createTing = false;
				return true;
			};

			MallOpen.getRegAutoCloneUrl = function(){
				var cloneUrl = $.cookie("regAutoCloneUrl");
				if(cloneUrl !== null){//把cookie清除掉
					$.cookie("regAutoCloneUrl", null, {
						domain: 'fkw.com'
					});
				}
				return cloneUrl;
			};

			//初始化波浪动画
			MallOpen.mWave = (function(){
				var $dom = $('.m-wave');

				function init(){
					start($dom.find('.wave-item.item1'), 150000);
					start($dom.find('.wave-item.item2'), 140000);
					start($dom.find('.wave-item.item3'), 30000);
				}
				function start($target, speed){
					var halfWidth = parseInt($target.width() / 2);
					$target.animate({'marginLeft': - halfWidth + 'px'}, speed, 'linear', roll);
					function roll(){
						$target.css('marginLeft', 0);
						$target.animate({'marginLeft': - halfWidth + 'px'}, speed, 'linear', roll);
					}
				}
				return {
					init: init
				}
			})();
			MallOpen.initEvent = function(){
				$("#mallNameInput").on("input",function(e){
					var mallNameInput = e.target.value || "";
					$("#inputSize").text(mallNameInput.length);
					if (Fai.top.aid % 2 === 1 && !Fai.top._oem && disCreateFirstSite){  //直销b类首次创建
						MallOpen.checkCreateMallButton(selectMallTypeValue);
					}else {
						MallOpen.checkCanCreateMall();
					}
				});
				//类型选择
				$(".createMall .mallTypeChooice").click(function(e){
					$(".createMall .mallTypeChooice").removeClass("checkedType");
					$(this).addClass("checkedType");
					MallOpen.checkCanCreateMall();
				})

				//创建小程序商城
				$(".msCreateMallAppletsBtn").click(function () {
					Site.allSiteLogDog(200933, 3);
					MallOpen.createMallSite(1);
				});
				//创建电脑商城
				$(".msCreateMallComputerBtn").click(function () {
					Site.allSiteLogDog(200933, 5);
					MallOpen.createMallSite(2);
				});
				//创建微商城
				$(".msCreateMallSmallBtn").click(function () {
					Site.allSiteLogDog(200933, 4);
					MallOpen.createMallSite(3);
				});

				$(".deleteLogo").click(function(e){
					$(".logoImg").attr("style","");
					MallOpen.logoImgInfo = {};
					$(this).hide();
					e.stopPropagation();
				})

				$(".msTargetRadio,.cloneLogo,.msTargetLabel").click(function(){
					$(".radioItem .msTargetRadio").removeClass("msTargetRadioCheck");
					$(this).parent(".radioItem").find(".msTargetRadio").addClass("msTargetRadioCheck");
					$("#goToCloneWeb").addClass("msCanCreateMallBtn");
				})

				$("#goToCloneWeb").click(function(){
					if(!$(this).hasClass("msCanCreateMallBtn")){
						return;
					}
					var targetRadio = $(".cloneMall .radioItem .msTargetRadioCheck");
					if(targetRadio.length > 0){
						var cloneWeb = $(targetRadio).attr("url") || "";
						if(cloneWeb == ""){
							MallOpen.ing("请先选择模板应用的站点。",true)
							return;
						}
						Fai.top.location = cloneWeb;
					}else{
						MallOpen.ing("请先选择模板应用的站点。",true)
					}

				})

			}
			MallOpen.ing = function(str,autoClosr){
				Fai.ing(str,autoClosr);
				$("#ing").css("z-index",　"100000");
			}

			//b类用户类型选择
			$("input[name='mallType']").click(function () { //将按钮设置为可用的样式
				selectMallTypeValue = $(this).val();
				MallOpen.checkCreateMallButton(selectMallTypeValue);
			});

		})(MallOpen, Site);
		(function(MallOpen, Site){
			
			//引入vue组件，就要用vue语法了
			MallOpen.mallOpenApp = new Vue({
				el: '#createMallSite',
				data: function(){
					return {
						mallType: -1,
						mallName: '',
						isVisible: true,
						isWantOpenMall: false,
						isWantCreateMallSite: true,
						isWantCloneMallSite: false,
						isCreateMallSiteSecondly:false//是否是二次创建站点的状态
					}
				},
				mounted: function(){
					var that = this;
					Site.Storage.$on("onCreateMallSite", function(){
						that.onCreateMallSite();
					});
				},
				methods: {
					onCreateMallSite: function(){
						var that = this;
						that.isWantCreateMallSite=true;
						$('.J_createMallSiteWrap').show();
						that.isCreateMallSiteSecondly=true;
						$(".createMallSite").show();
					},
					stopPropagation: function(event){
						event.stopPropagation();
						return false;
					},
					hideCreateMallSitePopup: function(){
						if(this.isCreateMallSiteSecondly){
							$(".createMallSite").hide();
						}
					},	
					getFormData: function(){
						return {
							mallName: this.mallName,
							mallType: this.mallType
						}
					},
					createMallSite: function(){
			
						if(!this.checkFormWithTip()){
							return;
						}

						//这里是同步处理，所以放到后面
						var isOK = MallOpen.createMallSite(0);  //1表示A类用户
						Vue.$eventBus.$emit("onCurrentSiteNameUpdated", this.mallName);//更新网站名称
						if (isOK) {
							Site.allSiteLogDog(200933, 1);
							$(".createMallSite").hide();
						}
					},
					checkFormWithTip: function(){

						if($.trim(this.mallName) == ""){
							MallOpen.ing("请填写商城名称");
							$("#mallName").focus();
							return false;
						}

						if(this.mallType === -1){
							MallOpen.ing("请选择你要搭建商城类型");
							return false;
						}
						
						return this.canSubmit;
					}
				},
				computed:{
					//按钮是否禁用
					canSubmit: function(){
						//名称填写了
						var isMallNameFill = (this.mallName !== "");
						var isMallTypeFill = (this.mallType !== -1);

						return isMallNameFill && isMallTypeFill;
					},
					btnText: function(){
						return '立即创建';
					}
				}
			});
		}(MallOpen, Site));
		// MallOpen.mWave.init();
		MallOpen.initEvent();

	}(MallOpen, Site));
	</script>

	


