						<div class="title">请问您还有其他完善产品的建议吗？</div>
						<div class="options advice">
							<input-component type="textarea" v-model="advice"></input-component>
						</div>
					</div>
					<div class="content" v-if="issue==2">
						<div class="title">您希望我们的产品提供哪些方便的操作？<span class="star">*</span><span class="warn" v-if="suTips">请提供您期望操作方便的地方</span></div>
						<div class="options advice">
							<input-component type="textarea" v-model="suggest" :maxlength="10000"></input-component>
						</div>
					</div>
				</div>
				<div class="renewalUser" v-if="renewalUser">
					<div class="suggest">
						<div class="title">我们关注到您是在网站版本到期后才进行付费，请问是什么原因呢？</div>
						<div class="options suggest">
							<input-component type="textarea" v-model="suggest" :maxlength="10000"></input-component>
						</div>
					</div>
				</div>
				<div class="btn_group" v-if="issue>0">
					<div class="submit_btn" @click="submit">提交</div>
					<div class="cancel_btn" @click="cancel">取消</div>
				</div>
			</div>
		</div>
	</div>
</div>
<script>
var Data = {
	isMobile : 0,
	timeDefault :2,
	siteTypes : [
		{ label: '电脑版', wid: 0 },
		{ label: '手机版', wid: 1 }
	],
	timeType : [
		{ label: '昨日',type: 2 },
		{ label: '最近7日', type: 3 },
		{ label: '最近30日', type: 4 }
	],
	siteShow : Fai.top.staffAuth && Fai.top.siteType != 2 && !Fai.top.manageClose,
	allowedSpider : Fai.top.allowedSpider,
	isCloseSite : Fai.top.isCloseSite,
	usedUv:0,
	allFreeUv: Fai.top.allFreeUv,
	warnFreeUv: Fai.top.warnFreeUv,
	isOem: Fai.top._oem,
	_siteVer: Fai.top._siteVer,
	buyMailSuffix: Fai.top.buyMailSuffix,
	showBuyMailTip: Fai.top.buyMailSuffix != '',
	//用户调研
	feedBackShow: false,
	aimAtUser: true,
	renewalUser: false,
	isConvenient: null,
	issue: 0,
	consume: -1,
	operation0: false,
	operation1: false,
	operation2: false,
	operation3: false,
	editContent: '',
	opTips: false,
	conTips: false,
	suTips: false,
	advice: '',
	suggest: '',
	sType: 0,
	mailSuffixABtest: Fai.top.aid % 2 == 0 ? '/marketing/mailMix.jsp' : '/marketing/mail.jsp?biz=32',
    mailUrl: Fai.top.httpStr +  Fai.top._portalHost ,
    blogs : Fai.top.blogs
};
//初始化VUE
(function(Fai){
	var action = "/ajax/statistics_h.jsp?cmd=timeData&siteId="+Fai.top.siteId;
	var allowedSpider = Fai.top.allowedSpider;
	var app = new Vue({
        el: '#jz_sitesurvey',
        data: Data,
        computed: {
        	persent: function(){
        		var  persent = this.usedUv / Fai.top.allFreeUv * 100;
        		if(persent >= 100){
        			persent = 100;        		}
        		return	parseInt(persent);
        	},
        	persent2: function(){
    			var circleLength = Math.floor(2 * Math.PI * 44);
	    		return circleLength * this.persent / 100 +",10000"; 
	    	},
	    	strokeColor: function(){
	    		if(this.persent < 20){
	    			return "#06c1ae";
	    		}else if(this.persent <= 60){
	    			return "#ff7214";
	    		}else{
	    			return "#ff9372";
	    		}
	    	} ,
	    	updateVersionUrl: function(){
	    		return Fai.top.httpStr +  Fai.top._portalHost + '/jump.jsp?t=10&d=100097&s=5_f=10&purchaseFrom=jzallsite&buySiteVer=true&siteSid=' + Fai.top.siteId;
	    	},
	    	updateWenAn: function(){
	    		var wenan = "立即升级，拥有无限流量";
	    		if(this.usedUv >= Fai.top.allFreeUv){
	    			wenan = "网站已无法访问，立即升级";
	    		}
	    		if(this.usedUv >= Fai.top.warnFreeUv && this.usedUv < Fai.top.allFreeUv){
	    			wenan = "网站将无法访问，立即升级";
	    		}
	    		return wenan;	
    		}    	
        },
        created: function(){
        	//链接曝光 by jser
            Site.allSiteLogDog(200623, 1);
            this.toPreUrl();
        },
        mounted: function(){
        	if(!this.isOem){
        		$.ajax({
					type: 'post',
					url: "/ajax/statistics_h.jsp?cmd=status&siteid="+Fai.top.siteId+"&aid=" + Fai.top.aid,
					timeout: 60000,
					error: function(){
						Fai.ing("系统错误。", false);
					},
					success: function(data){
						var result = $.parseJSON(data);
						var _this = app;
						if(result.rt > 0){
							_this.feedBackShow = true;
							_this.sType = result.rt;
							_this.aimAtUser = result.rt == 1 ? true : false;
							_this.renewalUser = result.rt == 2 ? true : false;
							_this.isConvenient = result.rt == 2 ? true : null;
							//调研页面曝光
							Site.logCorpDog(200630, 1);
						}
					}
				});
        	}
        },
        methods:{
			siteTypeChange : siteTypeChange,
			timeTypeChange : timeTypeChange,
			findMore : function(){
				Site.allSiteLogDog(200494, 13); 
				Site.showTopManageFrameFromJzallSite('manageFrameSiteStatOverview');
				//window.open(siteUrl + "?_manageSite=true&openmf=manageFrameSiteStatOverview");
			},
			onUpdateVersionButtonClick: function(){
				Site.allSiteLogDog(200623, 2);//点击 by jser
				if(Fai.top.usedUv <= Fai.top.warnFreeUv){
					Site.allSiteLogDog(200618, 7);
				}else if(Fai.top.usedUv > Fai.top.warnFreeUv && Fai.top.usedUv <= Fai.top.allFreeUv){
					Site.allSiteLogDog(200618, 9);
				}else if( Fai.top.usedUv >= 501){
					Site.allSiteLogDog(200618, 11);
				}
			},
			showOptions: function(){
				this.issue = this.isConvenient ? 1 : 2;
			},
			submit:function(){
				var data = {
					"location": 0,
					"aid": Fai.top.aid,
					"siteId": Fai.top.siteId,
					"isConvenient": Data.isConvenient,
					"sType": Data.sType
				}
				data.advice = this.isConvenient ? Fai.encodeHtml(Data.advice) : Fai.encodeHtml(Data.suggest);
				if(this.isConvenient){
					data.consume = this.consume;
					data.operation = "";
					this.conTips = this.consume == -1 ? true : false;
					this.opTips = !(this.operation0 || this.operation1 || this.operation2 || this.operation3) ? true : false;
					if(this.opTips || this.conTips){
						return;
					}
					if(this.operation0){
						data.operation += "复制模板网站 ";
					}
					if(this.operation1){
						data.operation += "替换网站图片 ";
					}
					if(this.operation2){
						data.operation += "编辑网站模板 ";
					}
					if(this.operation3){
						data.operation += this.editContent;
					}
					data.operation = Fai.encodeHtml(data.operation);
				}else{
					if($.trim(data.advice) == ""){
						this.suTips = true;
						return;
					}else{
						this.suTips = false;
					}
				}
				$.ajax({
					type: "post",
					url: "/ajax/statistics_h.jsp?cmd=feedback",
					data: data,
					timeout: 60000,
					error: function(){
						Fai.ing("提交失败，请稍后重试。", false);
					},
					success: function(data){
						app.feedBackShow = false;
						Fai.ing("提交成功，感谢您的反馈。", true);
					}
				});
			},
			cancel:function(){
				this.advice = '';
				this.suggest = '';
				this.isConvenient = null;
				this.issue = 0;
				this.consume = -1;
				this.operation0 = false;
				this.operation1 = false;
				this.operation2 = false;
				this.operation3 = false;
			},
			toPreUrl:function(){
				var $self = this;
				var key = "_preUrl";
				var value = $self.getCookie(key);
				if(value == null || value == ''){
					return;
				}
				var goDesignTipValue = localStorage.getItem("goDesignTip");
				if(goDesignTipValue){
					$self.setCookie(key, '', -1);
					return;
				}
				value = value.split('//')[1];
				var args = value.indexOf('/'); //获取链接参数
				var link = preSiteUrl + value.substring(args);
				//底部管理登录后的提示弹窗不再提醒优化
				var myCheckBox=[
					'<div style="margin-top:10px; color:#666">',
						'<input type="checkbox" onclick="goDesignCheckBoxClick(this);" style="width: 16px;height: 16px;background-color: #fff;-webkit-appearance:none;border: 1px solid #9f9f9f;border-radius: 2px;outline: none;margin: 0px; vertical-align: -2px;cursor: pointer;" /> 不再提醒',
					'</div>'
				];
				new Vue().$createDialog({
					confirmButton: {text: '前 往'},
					cancelButton: {text: '取 消'},
					content: '设计刚才访问的页面？' + myCheckBox.join(''),
					onConfirm: function() {
						Site.allSiteLogDog(200584, 4);
						$self.setCookie(key, '', -1);
						window.open(link);
					},
					onCancel: function(){
						$self.setCookie(key, '', -1);
					},
					onClose: function() {
						$self.setCookie(key, '', -1);
					}
				});
			},
			setCookie:function(key, value, life){
				var date = new Date();
				date.setTime(date.getTime() + (life * 24 * 60 * 60 * 1000));
				var expires = "expires=" + date.toUTCString();
				document.cookie = key + "=" + value + ";expires" + expires + ";path=/;domain=" + document.domain;
			},
			getCookie:function(key){
				var regx = '(^|;) ?' + key + '=([^;]*)(;|$)';
         		var value = document.cookie.match(regx);
          		return value ? value[2] : null;
			},
			onUpdateSitePop: function(){
				switch(Fai.top._siteVer){
					case 10: 
						Site.logCorpDog(200494, 21);
						break;
					case 25: 
						Site.logCorpDog(200494, 31);
						break;
					case 27: 
						Site.logCorpDog(200494, 22);
						break;
					case 30: 
						Site.logCorpDog(200494, 23);
						break;
				}
				window.open(getUpateVerUrl(14));
			},
			onUpdateSiteCrown: function(){
				Site.logCorpDog(200494, 24);
				window.open(getUpateVerUrl(259));
			},
			handleSiteDesgin: function(){
				Site.logCorpDog(200494, 25);
				Site.logCorpDog(200968, 1);
				var link = Fai.top.siteUrl + "?_fromModuleCenter=true";
				window.open(link);
			},
			showTopManageFrame: function(target){
				switch(target){
					case 'manageFrameSiteStatOverview':
						Site.logCorpDog(200494, 26);
						Site.logCorpDog(200968, 2);
						break;
					case 'manageFrameSiteImg':
						Site.logCorpDog(200494, 27);
						Site.logCorpDog(200968, 7);
						break;
					case 'manageFrameMemberManage':
						Site.logCorpDog(200494, 28);
						Site.logCorpDog(200968, 3);
						break;
					case 'manageFrameLanguage':
						Site.logCorpDog(200494, 29);
						Site.logCorpDog(200968, 24);
						break;
					case 'moduleAdvanced':
						Site.logCorpDog(200494, 30);
						break;
				}
				Site.showTopManageFrameFromJzallSite(target);
			},
			hideBuyMailTip : function(){
				this.showBuyMailTip = false;
				window.localStorage.setItem('hideBuyMailTip', true);
			},
			moreBlogs : function () {
				Site.allSiteLogDog(201245, 25); 
			    window.open(Fai.top.moreBlogsLink);
			},
			clickBlog : function (blog) {
				Site.allSiteLogDog(201245, blog.srcId+12); 
			}
        }
    });
	
	init();
	getTimeData();
	initUv();


	function setCookie (key, value, life) {
        var date = new Date();
        date.setTime(date.getTime() + (life * 24 * 60 * 60 * 1000));
        var expires = "expires=" + date.toUTCString();
        window.document.cookie = key + "=" + value + "; " + expires;
    }

    function getCookie(key) {
        var regx = '(^|;) ?' + key + '=([^;]*)(;|$)';
        var value = window.document.cookie.match(regx);
        return value ? value[2] : null;
    }

    function uvLimit(){
    	var cookieKey = Fai.top.aid + Fai.top.siteId + Data.isMobile;
    	if(getCookie(cookieKey)){
    		return;
    	}
    	var wenan = Data.isMobile == 0 ? "电脑" : "手机";
		//UV限制
	    if(Fai.top._siteVer == 10 && Fai.top.usedUv >= Fai.top.warnFreeUv && Fai.top.usedUv < Fai.top.allFreeUv){
	        new Vue().$createDialog({
	            content: '您的'+wenan+'网站月流量已达' + Fai.top.usedUv + 'UV，免费版每月流量为' + Fai.top.allFreeUv + 'UV，为了</br>保证网站正常访问，建议您升级网站版本',
	            confirmButton: {text: '升级网站'},
	            cancelButton: {text: '流量够用'},
	            onConfirm: function() {
	            	Site.allSiteLogDog(200618, 2);
	                window.open(Fai.top.httpStr +  Fai.top._portalHost + '/jump.jsp?t=10&d=100097&s=5_f=10&buySiteVer=true&siteSid=' + Fai.top.siteId)
	            },
	            onCancel: function() {},
	            icon: 'warning'
	        });
	        Site.allSiteLogDog(200618, 1);
	        Site.allSiteLogDog(200618, 4);
	        setCookie(cookieKey,true,1);
	    }else if(Fai.top._siteVer == 10 && Fai.top.usedUv >= Fai.top.allFreeUv){
	        new Vue().$createDialog({
	            content: '您的'+wenan+'网站月流量已达' + Fai.top.usedUv + 'UV，网站无法正常访问，</br>建议升级网站版本',
	            confirmButton: {text: '立即升级， 拥有无限流量'},
	            onConfirm: function() {
	            	Site.allSiteLogDog(200618, 3);
	                window.open(Fai.top.httpStr +  Fai.top._portalHost + '/jump.jsp?t=10&d=100097&s=5_f=10&buySiteVer=true&siteSid=' + Fai.top.siteId)
	            },
	            icon: 'warning'
	        });
	       Site.allSiteLogDog(200618, 1);
	       Site.allSiteLogDog(200618, 5);
	       setCookie(cookieKey,true,1);
	    }
    }

	function getcurrentTime(){  
	    var systemDate = new Date();  
	    // 获取当年
	    var year = systemDate.getFullYear();  
	    // 获取当月 （月+1是因为js中月份是按0开始的）
	    var month = systemDate.getMonth() + 1;  
	    if (month < 10) { // 如果月小于10，前面拼接0
	        month = '0' + month;  
	    }  
	    return (new Date([year, month, '01'].join('-')).getTime()) / 1000;  
  }

    function getCurrentMonthLast(){
		var date=new Date();
		var currentMonth=date.getMonth();
		var nextMonth=++currentMonth;
		var nextMonthFirstDay=new Date(date.getFullYear(),nextMonth,1);
		var oneDay=1000*60*60*24;
		return (new Date(nextMonthFirstDay-oneDay).getTime()) / 1000;
	}

	function initUv(){
		var params = '&typeQy=0' + '&isMobile= '+ Data.isMobile + '&siteLan=' + Fai.top.wid + '&startTime=' + getcurrentTime() + '&endTime=' + getCurrentMonthLast();
		var type = 0;
		$.ajax({
			type: 'post',
			url: action,
			data: params,
			timeout: 60000,
			async: false,//同步请求
			error: function(xhr, status, error){
				if (status == 'timeout') {
					Fai.ing("抱歉，数据量过大查询失败。请稍后再试");
				}
			},
			success: function(data){
				var result = $.parseJSON( data );
				if( result.success ){
					var timeData = result.timeData;
					var uvTotal = 0;
					//处理时间点上的数据
					if(timeData.length > 0){
						var dataTmp;
						for(var i = 0; i < timeData.length - 1; i++ ){
							dataTmp = timeData[i];
							uv = dataTmp.iVisitors;
							uvTotal += uv;
						}
						app.usedUv = uvTotal;
						Fai.top.usedUv = uvTotal;
						if(Fai.top.usedUv <= Fai.top.warnFreeUv){
							Site.allSiteLogDog(200618, 6);
						}else if(Fai.top.usedUv > Fai.top.warnFreeUv && Fai.top.usedUv <= Fai.top.allFreeUv){
							Site.allSiteLogDog(200618, 8);
						}else if(Fai.top.usedUv > Fai.top.allFreeUv){
							Site.allSiteLogDog(200618, 10);
						}
						uvLimit();
					}			
				}
				
			}
		});	
	}
	
	function init(){
		initHighChartsComponent();
		var isSpecialVer = ! false;
		if(isSpecialVer){
			Data.timeDefault = 3;//付费用户默认选择选中七天
		}else{
			Data.timeDefault = 2;//其他用户默认选择昨日
		}
	}
	
	function initHighChartsComponent(){
		//设置线性图的颜色 
		Highcharts.setOptions({ 
			colors: ['#468cfe', '#ffdb56', '#ff9239', '#ff4745', '#95cf22', '#6a4dff', '#0099cf', '#00a04f'] 
		});
	}
		
	function siteTypeChange(value){
		this.isMobile = value;
		getTimeData();
		initUv();
	}
	
	function timeTypeChange(value){
		this.timeDefault = value;
		getTimeData();
	}
	
	function getTimeData(){
		var type = Data.timeDefault;
		var params = '&typeQy=' + Data.timeDefault + '&isMobile= '+ Data.isMobile + '&siteLan=' + Fai.top.wid;
		if(type == 0){
			var startTimeButtonVal = $("#startTimeButton").val();
			if(startTimeButtonVal == "开始时间"){
				Fai.ing("请输入开始时间", true);
				return false;
			}
			
			var endTimeButtonVal = $("#endTimeButton").val();
			if(endTimeButtonVal == "结束时间"){
				Fai.ing("请输入结束时间", true);
				return false;
			}
			
			if(endTimeButtonVal<startTimeButtonVal){
				Fai.ing("结束时间不能小于开始时间", true);
				return false;
			}
			
			var tmpStartTimeDate = new Date(Date.parse(startTimeButtonVal.replace(/-/g,   "/")));
			var tmpStartTimestamp = tmpStartTimeDate.getTime()/1000;
			var tmpEndTimeDate = new Date(Date.parse(endTimeButtonVal.replace(/-/g,   "/")));
			var tmpEndTimestamp = tmpEndTimeDate.getTime()/1000;
	
			params += "&startTime=" + tmpStartTimestamp + "&endTime=" + tmpEndTimestamp;
		}
		
		params += '&R=' + Math.random();
		$.ajax({
			type: 'post',
			url: action,
			data: params,
			timeout: 60000,
			error: function(xhr, status, error){
				if (status == 'timeout') {
					Fai.ing("抱歉，数据量过大查询失败。请稍后再试");
				}
			},
			success: function(data){
				var result = $.parseJSON( data );
				if( result.success ){
					var timeData = result.timeData,
						xTimeList = result.xTimeList,
						textData,
						timeLineArr = [],
						series = [],
						dataArr = [],
						pvParam = {},
						uvParam = {},
						ipParam = {},
						visitCountParam = {},
						visitDepthParam = {},
						visitPageCountParam = {},
						visitTimeParam = {},
						jumpOutParam = {};
						pvParam.name = "PV";
						uvParam.name = "UV";
						uvParam.visible = false;
						ipParam.name = "IP";
						ipParam.visible = false;
						visitCountParam.name = "访问数";
						visitCountParam.visible = false;
						visitDepthParam.name = "平均浏览深度";
						visitDepthParam.visible = false;
						visitPageCountParam.name = "人均浏览页面数";
						visitPageCountParam.visible = false;
						visitTimeParam.name = "平均停留时长(秒)";
						visitTimeParam.visible = false;
						jumpOutParam.name = "跳出率";
						jumpOutParam.visible = false;
					xTimeList.sort(function(a,b){return a>b?1:-1});
					if(type == 1 || type == 2){	//今日、昨日
						for(var i = 0; i < xTimeList.length - 1; i++){
							var timeTmp = xTimeList[i];
							var dateTmp = new Date(timeTmp * 1000);
							var hour = dateTmp.getHours() + "点";
							timeLineArr.push(hour);
							dataArr[i] = 0;
						}
					}else if(type == 3 || type == 4){
						for(var i = 0; i < xTimeList.length; i++){
							var timeTmp = xTimeList[i];
							var dateTmp = new Date(timeTmp * 1000);
							var month = dateTmp.getMonth() + 1;
							if(month < 10){
								month = "0" + month;
							}
							var day = dateTmp.getDate();
							if(day < 10){
								day = "0" + day;	
							}
							timeLineArr.push(month + "-" + day);
							dataArr[i] = 0;
						}
					}else if(type == 0){
						var agoTime = result.agoTime;
						if(agoTime > 1){
							for(var i = 0; i < xTimeList.length; i++){
								var timeTmp = xTimeList[i];
								var dateTmp = new Date(timeTmp * 1000);
								var month = dateTmp.getMonth() + 1;
								if(month < 10){
									month = "0" + month;
								}
								var day = dateTmp.getDate();
								if(day < 10){
									day = "0" + day;	
								}
								timeLineArr.push(month + "-" + day);
								dataArr[i] = 0;
							}
						}else{
							for(var i = 0; i < xTimeList.length; i++){
								var timeTmp = xTimeList[i];
								var dateTmp = new Date(timeTmp * 1000);
								var hour = dateTmp.getHours() + "点";
								timeLineArr.push(hour);
								dataArr[i] = 0;
							}
						}
					}
					//处理时间点上的数据
					if(timeData.length > 0){
						textData = timeData[timeData.length - 1];
						var dataTmp, timeStamp, dateTmp, index, pv, uv, ip, visitCount, visitDepth, visitPageCount, visitTime, visitTimeAvr, reVisitCount, jumpOut;
						var pvTotal = 0, uvTotal = 0, ipTotal = 0, visitCountTotal = 0, revisitCountTotal = 0, visitDepthTotal, visitPageCountTotal, visitTimeTotal = 0, jumpOutTotal;
						var spiderGoogle = 0, spiderBaidu = 0, spiderOthers = 0;
						var pvData = dataArr.concat(),
							uvData = dataArr.concat(),
							ipData = dataArr.concat(),
							visitCountData = dataArr.concat(),
							visitDepthData = dataArr.concat(),
							visitPageCountData = dataArr.concat(),
							visitTimeData = dataArr.concat(),
							jumpOutData = dataArr.concat();
						var newVisitor = 0, oldVisitor = 0;
						for(var i = 0; i < timeData.length - 1; i++ ){
							dataTmp = timeData[i];
							timeStamp = dataTmp.logTimeStamp;
							index = xTimeList.indexOf(timeStamp);
							pv = dataTmp.pv;
							uv = dataTmp.iVisitors;
							ip = dataTmp.iIp;
							visitCount = dataTmp.visitCount? dataTmp.visitCount : 0;
							visitDepth = (visitCount == 0? 0 : pv/visitCount);
							visitDepth = parseFloat(visitDepth.toFixed(2));
							visitPageCount = (uv == 0? 0 : pv/uv);
							visitPageCount = parseFloat(visitPageCount.toFixed(2));
							visitTime = dataTmp.visitTime? dataTmp.visitTime : 0;
							visitTimeAvr = (visitCount == 0? visitTime : visitTime/visitCount);
							visitTimeAvr = parseFloat((visitTimeAvr/1000).toFixed(2));
							reVisitCount = dataTmp.revisitCount? dataTmp.revisitCount : 0;
							jumpOut = 1 - (visitCount == 0? 1 : reVisitCount/visitCount);
							jumpOut < 0 ? jumpOut = 0 : jumpOut;
							jumpOut = parseFloat(jumpOut.toFixed(2));
							newVisitor += (dataTmp.newVisitor? dataTmp.newVisitor : 0);
							oldVisitor += (dataTmp.oldVisitor? dataTmp.oldVisitor :0);
							pvData[index] = pv;
							uvData[index] = uv;
							ipData[index] = ip;
							visitCountData[index] = visitCount;
							visitDepthData[index] = visitDepth;
							visitPageCountData[index] = visitPageCount;
							visitTimeData[index] = visitTimeAvr;
							jumpOutData[index] = jumpOut;
							pvTotal += pv;
							uvTotal += uv;
							ipTotal += ip;
							visitCountTotal += visitCount;
							revisitCountTotal += reVisitCount;
							visitTimeTotal += visitTime;
							
							spiderGoogle += dataTmp.spiderGoogle;
							spiderBaidu += dataTmp.spiderBaidu;	
							spiderOthers += dataTmp.spiderOthers;
						}
						pvParam.data = pvData;
						uvParam.data = uvData;
						ipParam.data = ipData;
						visitCountParam.data = visitCountData;
						visitDepthParam.data = visitDepthData;
						visitPageCountParam.data = visitPageCountData;
						visitTimeParam.data = visitTimeData;
						jumpOutParam.data = jumpOutData;
						
						textData.newVisitor = newVisitor;
						textData.oldVisitor = oldVisitor;
						
						$("#pv").text(pvTotal);
						$("#uv").text(uvTotal);
						$("#ip").text(ipTotal);
						$("#visitCount").text(visitCountTotal);
						visitDepthTotal  = (visitCountTotal == 0? 0 : pvTotal/visitCountTotal);
						visitDepthTotal = parseFloat(visitDepthTotal.toFixed(2));
						visitPageCountTotal = (uvTotal == 0? 0 : pvTotal/uvTotal);
						visitPageCountTotal = parseFloat(visitPageCountTotal.toFixed(2));
						visitTimeTotalAvr = (visitCountTotal == 0? 0 : visitTimeTotal/visitCountTotal);
						//处理时间格式的显示
						var visitTimeStr = "";
						var visitTimeSecondAvr = parseInt(visitTimeTotalAvr/1000%60);
						var visitTimeMinuteAvr = parseInt(visitTimeTotalAvr/60000%60);
						var visitTimeHourAvr = parseInt(visitTimeTotalAvr/3600000);
						if(visitTimeHourAvr > 0){
							if(visitTimeHourAvr < 10){
								visitTimeStr += "0" + visitTimeHourAvr + ":";
							}else{
								visitTimeStr += visitTimeHourAvr + ":";
							}
						}else{
							visitTimeStr += "00:";
						}
						if(visitTimeMinuteAvr > 0){
							if(visitTimeMinuteAvr < 10){
								visitTimeStr += "0" + visitTimeMinuteAvr + ":";
							}else{
								visitTimeStr += visitTimeMinuteAvr + ":";
							}
						}else{
							visitTimeStr += "00:"
						}
						if(visitTimeSecondAvr < 10){
							visitTimeStr += "0" + visitTimeSecondAvr;
						}else{
							visitTimeStr += visitTimeSecondAvr;
						}
						$("#visitDepth").text(visitDepthTotal);
						$("#viewCount").text(visitPageCountTotal);
						$("#viewTime").text(visitTimeStr);
						if(visitCountTotal == 0){
							$("#jumpOut").text("0%");
						}else{
							jumpOutTotal = 1 - (visitCountTotal == 0? 0 : revisitCountTotal/visitCountTotal);
							jumpOutTotal = jumpOutTotal < 0? 1 : jumpOutTotal;
							jumpOutTotal = (jumpOutTotal * 100).toFixed(0);
							$("#jumpOut").text(jumpOutTotal + "%");
						}
						textData.spiderBaidu = spiderBaidu;
						textData.spiderGoogle = spiderGoogle;
						textData.spiderOthers = spiderOthers;
					}			
					series.push(pvParam);
					series.push(uvParam);
					series.push(ipParam);
					if(allowedSpider){
						series.push(visitCountParam);
						series.push(visitDepthParam);
						series.push(visitPageCountParam);
						series.push(visitTimeParam);
						series.push(jumpOutParam);
					}
					drawChartContent(timeLineArr, series);
					$(window).trigger("resize");
				}
				
			}
		});	
	}
	
	function drawChartContent(timeLineArr, series){
		$('.chartContent2').highcharts({
			plotOptions: {
				series: {
					events: {
						legendItemClick: function(e) {
							var index = this.index;
							var series = this.chart.series;
							if(!series[index].visible) {
								for(var i=0;i<series.length;i++) {
									var s = series[i];
									i===index ? s.show() : s.hide();
								}
							}
	
							// 如果显示跳出率，把Y轴上限置为1
							if (index == 7) {
								this.chart.yAxis[0].setExtremes(null, 1);
							} else {
								this.chart.yAxis[0].setExtremes(null, null);
							}
							return false;
						}
					}
				}
			},
			title: {
				text: '',
			},		
			xAxis: {
				categories: timeLineArr
			},
			yAxis: {
				title: {
					text: ''
				},
				plotLines: [{
					value: 0,
					width: 1,
					color: '#808080'
				}]
			},
			credits: {
				enabled:false
			},
			tooltip: {
				valueSuffix: ''
			},
			series: series
		});
	}

	function getUpateVerUrl(productId){

        var querys = [];
        querys.push("t=10");
        querys.push("buySiteVer=true");
        querys.push("siteVersion=" + productId);
        querys.push("siteSid=" + Fai.top.siteId);

        if(Fai.top._proSiteQueryStr !== undefined && Fai.top._proSiteQueryStr !==""){
            querys.push(Fai.top._proSiteQueryStr);
        }

        var url = Fai.top.httpStr +  Fai.top._portalHost + "/jump.jsp?"+ querys.join('&');
        return url;
    }
	
})(Fai)
</script>
