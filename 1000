            <!-- 老用户回馈优惠券 -->
            <div class='jz_renewRemind'  v-if='isShowRepeatBuyRemind'>
                <div class='renewRemindLeft'>
                    <span class='renewCoupon'></span>
                    <span class='renewText'>回馈老用户，特赠一张<span class='renewRepeatBuyWarn'>100元</span>{{bizName}}现金券<span class='renewTips'>（现金券仅<span class='renewRepeatBuyWarn'>7天</span>有效，期待你继续使用我们的产品）</span></span>
                </div>
                <div class='renewRemindRight'>
                    <button class='renewBtn' onclick='event.cancelBubble = true' @click='clickToRepeat'>
                        <span class='jumpto'>去使用</span>
                    </button>
                </div>
            </div>

            <div :class="{fk_mall_panel:true}" id="fk_mall_panel">
                <div class="fk_mall_panel_wrap">
                    
                    <div class="fk_mall_panel_new_left">
                        <!-- UI改造这块 -->

                        
                        <biz-site-new-detail-component :info="siteDetailInfo"></biz-site-new-detail-component>
                        <biz-model-new-component :info="siteDetailInfo"></biz-model-new-component>
                        
                        <!-- 基础功能和营销功能 -->
                        <biz-basic-marketing-component-b></biz-basic-marketing-component-b>

                    </div>
                    <!-- 分销右侧不出现 -->
                    <div class="fk_mall_panel_right fk_mall_panel_new_right">
                        <biz-func-questionnaire-component></biz-func-questionnaire-component>
                        <!-- 功能投票 -->
                        <biz-func-vote-component :info="siteDetailInfo"></biz-func-vote-component>
                        <!-- 帮助中心 -->
                        <biz-blog-component></biz-blog-component>
                        <!-- 官方动态 -->
                        <biz-official-news-component :list="gData.topBarUpdateList"></biz-official-news-component>
                        <!-- 使用排行 -->
                        <biz-use-rank-component></biz-use-rank-component>

                    </div>
                    <biz-topbtn-component></biz-topbtn-component>
                    <!-- 侧边栏 -->
                    <!-- <biz-sidebar-component></biz-sidebar-component> -->
                    
                </div>

            </div>

        </div>

        <div class='sevenExtPanel' v-if="false">
            <div class='sevenPanelInner'>
                <p class='sevenWz'>登陆满七天送短信包现金券</p>
                <p class='sevenXq' @click="createDialogLoginTips">了解详情</p>
            </div>
        </div>

        <transition name='fade'>
            <div class='wxQrCodeExtPanel' id="wxQrCodeExtPanel" v-if="codeShow">
                <div class='wxQrCodePanelInner'>
                    <div class='fkWXQrExCode'><span class='fkWXQrExLogo'></span></div>
                    <p class='fkWXQrCodeTitle'>关注微信号</p>
                    <p class='fkWXQrCodeDisc'>随时掌握最新信息</p>
                    <span class="wxQrCodeClose" @click='wxCodeClose'></span>
                </div>
            </div>
        </transition>
        <transition name='fade'>
            <div class='proAdvPanel' id="proAdvPanel" v-if="advShow">
                <div class='proAdvPanelInner'>
                    <div class='proAdvImage' @click='clickProImage'></div>
                    <span class="proAdvClose" @click='proAdvClose'></span>
                </div>
            </div>
        </transition>
        <transition name='fade'>
            <div class='couponAdvPanel' v-if="couponGuide">
                <div class='couponAdvPanelInner'>
                    <div class='couponAdvImage' :class="couponType == 274 ? 'coupon_100': couponType == 276 ? 'coupon_200': 'coupon_400'">
                        <div class="couponAdvBtn" @click='clickCouponImage'></div>
                    </div>
                    <span class="couponAdvClose" @click='couponAdvClose'></span>
                </div>
            </div>
        </transition>

        <div v-if='manageClose&&closeDesign&&(!isCloseSite)' class='closeDiv'>
            <div class='closeSiteImg'>
                <img src="//jzas.faisys.com/image/siteClose_gray.png"/>
            </div>
            <div class='rzFont'>您没有管理权限，请联系超级管理员</div>
        </div>

        <div v-if='isCloseSite' class='closeDiv'>
            <div class='closeSiteImg'>
                <img src="//jzas.faisys.com/image/siteClose.png"/>
            </div>
            <div v-if='isOem'>
                <template v-if='closeReason == "超过试用期"'>
                    <div style="font-size: 24px; color:#444; margin-top: 12px ">该站点已经关闭！</div>
                    <div style="font-size: 16px; color: #888; margin-top: 25px">关闭原因：超过试用期。</div>
                </template>
                <template v-else>
                    <div class='rzFont'>{{closeReason}}</div>
                    <button-component v-if='isCloseReview' class='btnsurvey retOpenBtn' type='global-oper' @click='retOpenSite'>前往电脑版</button-component>
                </template>
            </div>
            <div v-else>
                <div class='rzFont'>{{closeReason}}</div>
                <button-component v-if='closeReason == "重新开通后，可继续免费使用网站"' class='btnsurvey retOpenBtn' type='global-oper' @click='retOpenSite'>重新开通</button-component>
                <button-component v-if='closeReason != "重新开通后，可继续免费使用网站"' class='btnsurvey retOpenBtn' type='global-oper' @click='retOpenSite'>申诉</button-component>
            </div>
        </div>
    </div>
    
    <div id='jz_content' class='jz_content updateV2' >
        <!-- websiteCapacity.jsp.inc不需要在页面展示了
        <div id='jz_shopsurvey' class='shopSurvey' v-if='shopShow'  v-cloak>
	<div class="jz_feedBack" v-if="feedBackShow">
		<div class="fbDiv">
			<span class="fb1"></span>
			<span class="fb2">您好，为了之后更好地为您提供服务，请给我们用户专家留下意见哦</span>
		</div>
		<div class="jz_sitesurvey">
			<div class="level">
				<div class="aimAtUser" v-if="aimAtUser">
					<div class="title">我们的产品是否有助于您更方便的搭建网站？</div>
					<div class="options">
						<radio-group-component v-model="isConvenient" v-on:on-change="showOptions">
							<radio-component :label="true">是</radio-component>
							<radio-component :label="false">否</radio-component>
						</radio-group-component>
					</div>
					<div class="content" v-if="issue==1">
						<div class="title">您进行过哪些网站操作？【多选】<span class="star">*</span><span class="warn" v-if="opTips">请选择以下选项</span></div>
						<div class="options operation">
							<div class="checkBox">
								<checkbox-component :true-value="true" :false-value="false" v-model="operation0">复制模板网站</checkbox-component>
								<checkbox-component style="margin-left: 20px;" :true-value="true" :false-value="false" v-model="operation1">替换网站图片</checkbox-component>
								<checkbox-component style="margin-left: 20px;" :true-value="true" :false-value="false" v-model="operation2">编辑网站模板</checkbox-component>
								<checkbox-component style="margin-left: 20px;" :true-value="true" :false-value="false" v-model="operation3">其他</checkbox-component>
							</div>
							<input-component v-if="operation3" pattern="width:625px;" v-model="editContent" :maxlength="200"></input-component>
						</div>
						<div class="title">您当时搭建网站用了多长时间呢？<span class="star">*</span><span class="warn" v-if="conTips">请选择以下选项</span></div>
						<div class="options">
							<radio-group-component v-model="consume" >
								<radio-component :label="1">30分钟内</radio-component>
								<radio-component :label="2">30分钟-60分钟</radio-component>
								<radio-component :label="3">60分钟-1天内</radio-component>
								<radio-component :label="4">超过1天</radio-component>
							</radio-group-component>

						</div>
						<div class="title">请问您还有其他完善产品的建议吗？</div>
						<div class="options advice">
							<input-component type="textarea" v-model="advice"></input-component>
						</div>
					</div>
					<div class="content" v-if="issue==2">
						<div class="title">您希望我们的产品提供哪些方便的操作？<span class="star">*</span><span class="warn" v-if="suTips">请提供您期望操作方便的地方</span></div>
						<div class="options advice">
							<input-component type="textarea" v-model="suggest" :maxlength="10000"></input-component>
						</div>
					</div>
				</div>
				<div class="renewalUser" v-if="renewalUser">
					<div class="suggest">
						<div class="title">我们关注到您是在网站版本到期后才进行付费，请问是什么原因呢？</div>
						<div class="options suggest">
							<input-component type="textarea" v-model="suggest" :maxlength="10000"></input-component>
						</div>
					</div>
				</div>
				<div class="btn_group" v-if="issue>0">
					<div class="submit_btn" @click="submit">提交</div>
					<div class="cancel_btn" @click="cancel">取消</div>
				</div>
			</div>
		</div>
	</div>
</div>

<script>
(function(Fai){
	
	var vueData = {};
	var wid = Fai.top.siteId * 100;
	var defaultCn = Fai.top.defaultCn;
	var TYPE_SITE = 0;
	var TYPE_MOBI = 1;
	var TYPE_WXAPP = 3;
	var currencyVal = "￥";
	
	vueData.filterDate = 7;
	vueData.siteType = -1;
	vueData.siteTypeList = [{"val":-1,"name":"所有类型"},{"val":TYPE_SITE,"name":"电脑版"},{"val":TYPE_MOBI,"name":"手机版"},{"val":TYPE_WXAPP,"name":"小程序"}];
	vueData.defaultType = -1;
	vueData.wid = wid;
	vueData.defaultCn = defaultCn;
	vueData.currencyVal = currencyVal;
    vueData.resRoot = Fai.top.resRoot;
	
	vueData.visitor = 0;
	vueData.settleBuyer = 0;
	vueData.settleCount = 0;
	vueData.settleMoney = 0.0;
	vueData.payBuyer = 0;
	vueData.payCount = 0;
	vueData.payMoney = 0.0;
	vueData.settleRate = 0.0;
	vueData.settle_payRate = 0.0;
	vueData.payRate = 0.0;	
	vueData.shopShow = Fai.top.mallAuth && Fai.top.mallShow;
	vueData.isCloseSite = Fai.top.isCloseSite,
	
	vueData.fkMoney = 0;//付款金额
	vueData.fkOrder = 0;//付款订单数
	vueData.fkBuyer = 0;//付款买家数
	vueData.fkGoods = 0;//付款商品件数
	
	vueData.timeType = [
		{ label: '昨日',type: -1 },
		{ label: '最近7日', type: 7 },
		{ label: '最近30日', type: 30 }
	];
	vueData.oem = Fai.top._oem;
	vueData.marketingManageAuth = Fai.top.marketingManageAuth;

	//调研弹窗
	vueData.feedBackShow = false;
	vueData.aimAtUser = true;
	vueData.renewalUser = false;
	vueData.isConvenient = null;
	vueData.issue = 0;
	vueData.consume = -1;
	vueData.operation0 = false;
	vueData.operation1 = false;
	vueData.operation2 = false;
	vueData.operation3 = false;
	vueData.editContent = '';
	vueData.opTips = false;
	vueData.conTips = false;
	vueData.suTips = false;
	vueData.advice = '';
	vueData.suggest = '';
	vueData.sType = 0;
	vueData.isFreeUser = false;
	
	//初始化VUE
	var app = new Vue({
		el: '#jz_shopsurvey',
		data: vueData,
		created: function(){
            //this.toPreUrl();
        },
        mounted: function(){
        	if(!this._oem){
        		$.ajax({
					type: 'post',
					url: "/ajax/statistics_h.jsp?cmd=status&siteid="+Fai.top.siteId+"&aid=" + Fai.top.aid,
					timeout: 60000,
					error: function(){
						Fai.ing("系统错误。", false);
					},
					success: function(data){
						var result = $.parseJSON(data);
						var _this = app;
						if(result.rt > 0){
							_this.feedBackShow = true;
							_this.sType = result.rt;
							_this.aimAtUser = result.rt == 1 ? true : false;
							_this.renewalUser = result.rt == 2 ? true : false;
							_this.isConvenient = result.rt == 2 ? true : null;
							//调研页面曝光
							Site.logCorpDog(200630, 1);
						}
					}
				});
        	}
        	this.showWelfareTab();
        },
		methods: {
			siteTypeChange2: function(value){
				siteType = value;
				getTimeData();
			},
			timeTypeChange : function(value){
				filterDate = value;
				getTimeData();
			},
			findMore : function(){
				Site.allSiteLogDog(200494, 14); 	
				this.showTopManageFrame('manageFrameTransactionAnalysis');
				//window.open(siteUrl + "?_manageSite=true&openmf=manageFrameTransactionAnalysis");
			},
            showTopManageFrame: function (target) {
            	if((target == 'manageFrameCoupon' || target == 'manageFramePromotion' || target == 'manageFrameSecKill' || target == 'manageFramePromoter' || target == 'manageFrameMarketingCenter') && !this.marketingManageAuth){
            		Fai.ing("你暂无操作权限", true);
            		return;
            	}

                switch (target){
                    case 'manageFramePromotion':
                    case 'manageFramePromoter':
                        Site.allSiteLogDog(200695, 1);
                        break;
                    case 'manageFrameCoupon':
                    case 'manageFrameSecKill':
                        Site.allSiteLogDog(200695, 2);
                        break;
                    case 'manageFrameMarketingCenter':
                        Site.allSiteLogDog(200695, 3);
                        break;
                    default: break;
                }

                Site.showTopManageFrameFromJzallSite(target);
            },
            showOptions: function(){
				this.issue = this.isConvenient ? 1 : 2;
			},
			submit:function(){
				var data = {
					"location": 0,
					"aid": Fai.top.aid,
					"siteId": Fai.top.siteId,
					"isConvenient": vueData.isConvenient,
					"sType": vueData.sType
				}
				data.advice = this.isConvenient ? Fai.encodeHtml(vueData.advice) : Fai.encodeHtml(vueData.suggest);
				if(this.isConvenient){
					data.consume = this.consume;
					data.operation = "";
					this.conTips = this.consume == -1 ? true : false;
					this.opTips = !(this.operation0 || this.operation1 || this.operation2 || this.operation3) ? true : false;
					if(this.opTips || this.conTips){
						return;
					}
					if(this.operation0){
						data.operation += "复制模板网站 ";
					}
					if(this.operation1){
						data.operation += "替换网站图片 ";
					}
					if(this.operation2){
						data.operation += "编辑网站模板 ";
					}
					if(this.operation3){
						data.operation += this.editContent;
					}
					data.operation = Fai.encodeHtml(data.operation);
				}else{
					if($.trim(data.advice) == ""){
						this.suTips = true;
						return;
					}else{
						this.suTips = false;
					}
				}
				$.ajax({
					type: "post",
					url: "/ajax/statistics_h.jsp?cmd=feedback",
					data: data,
					timeout: 60000,
					error: function(){
						Fai.ing("提交失败，请稍后重试。", false);
					},
					success: function(data){
						app.feedBackShow = false;
						Fai.ing("提交成功，感谢您的反馈。", true);
					}
				});
			},
			cancel:function(){
				this.advice = '';
				this.suggest = '';
				this.isConvenient = null;
				this.issue = 0;
				this.consume = -1;
				this.operation0 = false;
				this.operation1 = false;
				this.operation2 = false;
				this.operation3 = false;
			},
			toPreUrl:function(){
				var $self = this;
				var key = "_preUrl";
				var value = $self.getCookie(key);
				if(value == null || value == ''){
					return;
				}
				var goDesignTipValue = localStorage.getItem("goDesignTip");
				if(goDesignTipValue){
					$self.setCookie(key, '', -1);
					return;
				}
				var end = value.indexOf(".");
				var acct = value.substring(0, end);
				var arrUrl = value.split("//");
				var start = arrUrl[1].indexOf("/");
				var link = acct + "." + Fai.top.jzHomeUrl + arrUrl[1].substring(start);
				//底部管理登录后的提示弹窗不再提醒优化
				var myCheckBox=[
					'<div style="margin-top:10px; color:#666">',
						'<input type="checkbox" onclick="goDesignCheckBoxClick(this);" style="width: 16px;height: 16px;background-color: #fff;-webkit-appearance:none;border: 1px solid #9f9f9f;border-radius: 2px;outline: none;margin: 0px; vertical-align: -2px;cursor: pointer;" /> 不再提醒',
					'</div>'
				];
				new Vue().$createDialog({
					confirmButton: { text: '前 往' },
					cancelButton: { text: '取 消' },
					content: '设计刚才访问的页面？' + myCheckBox.join(''),
					onConfirm: function() {
						$self.setCookie(key, '', -1);
						window.open(link);
					},
					onCancel: function(){
						$self.setCookie(key, '', -1);
					},
					onClose: function() {
						$self.setCookie(key, '', -1);
					}
				});
			},
			showWelfareTab: function(){
				if(Fai.top._siteVer != 11){
					this.isFreeUser = true;
					return;
				}
				var key = "_showTab";
				var value = this.getCookie(key);
				if(value != null && value == '1'){
					this.isFreeUser = true;
					value = '0';
				}else{
					this.isFreeUser = false;
					value = '1';
					Site.logCorpDog(200806, 1);
				}
				this.setCookie(key, value, 30);
			},
			clickMemberStored: function(){
				Site.allSiteLogDog(200928, 2);
			},
			setCookie:function(key, value, life){
				var date = new Date();
				date.setTime(date.getTime() + (life * 24 * 60 * 60 * 1000));
				var expires = "expires=" + date.toUTCString();
				document.cookie = key + "=" + value + ";expires" + expires + ";path=/;domain=" + document.domain;
			},
			getCookie:function(key){
				var regx = '(^|;) ?' + key + '=([^;]*)(;|$)';
         		var value = document.cookie.match(regx);
          		return value ? value[2] : null;
			}
		}
	});
	
	init();
		
	function getTimeData(){
		
		//是否默认情况
		// var isDefault = app.filterDate == 7 && app.siteType == -1;
		var isDefault = true;//默认拿7天的用户数据 by jser

		var params = "&type="+app.filterDate;//7
		params += '&lan=' + Fai.top.wid;
		params += '&siteType=' + app.siteType;//-1
		params += '&R=' + Math.random();


		
		var cacheKey = "_mallStatTransaction_" + Fai.top.aid + "_" +Fai.top.wid;
		var cacheResultObj1 = sessionStorage.getItem(cacheKey);//缓存默认数据
		if(isDefault && cacheResultObj1 && cacheResultObj1.length > 0){
			cacheResultObj1 = $.parseJSON(cacheResultObj1);
			initDataFromAjaxResult1(cacheResultObj1);
		}else{
			$.ajax({
				type: 'post',
				url: '/ajax/mallStat_h.jsp?cmd=getTimeData_transaction&siteId='+Fai.top.siteId,
				data: params,
				timeout: 30000,
				error: function(xhr, status, error){
					if (status == 'timeout') {
						Fai.ing("抱歉，数据量过大查询失败。请稍后再试");
					}
				},
				success: function(data){
					var result = $.parseJSON(data);
					if(result.success){
						if(isDefault){//缓存默认数据
							sessionStorage.setItem(cacheKey, $.toJSON(result));
						}
						initDataFromAjaxResult1(result);
					}
				}
			});
		}

		var cacheKey2 = "_mallStatOverview_" + Fai.top.aid + "_" +Fai.top.wid;
		var cacheResultObj2 = sessionStorage.getItem(cacheKey2);//缓存默认数据
		if(isDefault && cacheResultObj2 && cacheResultObj2.length > 0){
			cacheResultObj2 = $.parseJSON(cacheResultObj2);
			initDataFromAjaxResult2(cacheResultObj2);
			
			
			$("#fkMoney").text(vueData.fkMoney);
			$("#fkOrder").text(vueData.fkOrder);
			$("#fkBuyer").text(vueData.fkBuyer);
			$("#fkGoods").text(vueData.fkGoods);
		}else{
			$.ajax({
				type: 'post',
				url: '/ajax/mallStat_h.jsp?cmd=getTimeData_mallStat&siteId='+Fai.top.siteId,
				data: params,
				timeout: 60000,
				error: function(xhr, status, error){
					if (status == 'timeout') {
						Fai.ing("抱歉，数据量过大查询失败。请稍后再试");
					}
				},
				success: function(data){
					var result = $.parseJSON(data);
					if(result.success){
						if(isDefault){//缓存默认数据
							sessionStorage.setItem(cacheKey2, $.toJSON(result));
						}
						initDataFromAjaxResult2(result);
					}
					$("#fkMoney").text(vueData.fkMoney);
					$("#fkOrder").text(vueData.fkOrder);
					$("#fkBuyer").text(vueData.fkBuyer);
					$("#fkGoods").text(vueData.fkGoods);
				}
			});
		}
	}

	function initDataFromAjaxResult1(result){
		var resData = result.data||{};
	
		vueData.visitor 		= resData.visitor || 0;
		vueData.settleBuyer 	= resData.settleBuyer || 0;
		vueData.settleCount 	= resData.settleCount || 0;
		vueData.settleMoney 	= resData.settleMoney || 0.0;
		vueData.payBuyer 		= resData.payBuyer || 0;
		vueData.payCount 		= resData.payCount || 0;
		vueData.payMoney 		= resData.payMoney || 0.0;
		vueData.settleRate 		= resData.settleRate || 0.0;
		vueData.settle_payRate 	= resData.settle_payRate || 0.0;
		vueData.payRate 		= resData.payRate || 0.0;
	}

	function initDataFromAjaxResult2(result){
		var timeData = result.timeData;
		var xTimeList = result.xTimeList;

		var fkMoneyParam = {name:'付款金额',data:[]};
		var fkOrderParam = {name:'付款订单数',data:[]};
		var fkBuyerParam = {name:'付款买家数',visible: false,data:[]};
		var fkGoodsParam = {name:'付款商品件数',visible: false,data:[]};

		if(timeData.length > 0 && xTimeList.length>0){
			$.each(xTimeList,function(i,n){
				fkMoneyParam.data.push(timeData[i].fkMoney);
				fkOrderParam.data.push(timeData[i].fkOrder);
				fkBuyerParam.data.push(timeData[i].fkBuyer);
				fkGoodsParam.data.push(timeData[i].fkGoods);
			});
		}

		var seriesList = [];
		seriesList.push(fkMoneyParam);
		seriesList.push(fkOrderParam);
		seriesList.push(fkBuyerParam);
		seriesList.push(fkGoodsParam);

		var totalData = result.totalData || {};
		totalData.fkMoney = totalData.fkMoney || 0.0;
		totalData.fkOrder = totalData.fkOrder || 0;
		totalData.fkBuyer = totalData.fkBuyer || 0;
		totalData.fkGoods = totalData.fkGoods || 0;

		initData(totalData,xTimeList,seriesList);
	}
	
	function init(){
		getTimeData();
        Site.allSiteLogDog(200699, 1);
        logEnterManageTime();
	}
	
	function initData(totalData,xTimeList,seriesList){
		app.fkMoney = totalData.fkMoney;
		app.fkOrder = totalData.fkOrder;
		app.fkBuyer = totalData.fkBuyer;
		app.fkGoods = totalData.fkGoods;
	}

	function logEnterManageTime(){
		if(!Fai.top._oem){
			$.ajax({
				type: 'post',
				url: "/ajax/jzallsite_h.jsp?siteId=" + Fai.top.siteId + "&cmd=logEnterManageTime",
				timeout: 60000,
				error: function(){},
				success: function(){}
			});
		}
	}

})(Fai)
</script>
