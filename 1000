<script type="text/javascript">
    $(function(){
        $("#upload-logo").click(function () {
            if (Fai.top._oem){
                return false;
            }
            var popUpDom =
                '<div id="jz_logopopup_shadow">' +
                '<div class="jz_logopopup">' +
                '            <div class="logopopup_header">' +
                '                <label>编辑Logo</label>' +
                '                <span class="logopopup_close_img"></span>' +
                '            </div>' +
                '            <div class="logopopup_body">' +
                '                <span id="logopopup_localup">' +
                '                    <p class="localup_img"></p>' +
                '                    <p class="localup_txt">从本地上传</p>' +
                '                </span>' +
                '                <span id="logopopup_makelogo">' +
                '                    <p class="makelogo_img"></p>' +
                '                    <p class="makelogo_txt">定制LOGO</p>' +
                '                </span>' +
                '            </div>' +
                '        </div>' +
                '</div>';
            window.top.$("body").append(popUpDom);
            Site.allSiteLogDog(200494, 32);

            createHtml5Upload();
            //定制logo
            window.top.$("#logopopup_makelogo").click(function () {
                Site.allSiteLogDog(200494, 34);
                var openUrl = Fai.top.httpStr + Fai.top._sjHost;
                if (Fai.top.isMall){
                    openUrl += "/designer/designBuyPage.jsp?productId=565&flag=JzLogo";//暂时不区分jz和mall
                } else {
                    openUrl += "/designer/designBuyPage.jsp?productId=565&flag=JzLogo";
                }
                window.open(openUrl, "_blank");
            });
            //点击本地上传log
            window.top.$("#logopopup_localup").click(function () {
                Site.allSiteLogDog(200494, 33);
            });

            // 关闭弹窗
            window.top.$(".logopopup_close_img").click(function () {
                window.top.$("#jz_logopopup_shadow").remove();
            });
        });

        if (Fai.top._oem){
            if('upload' in window.XMLHttpRequest.prototype){
                createHtml5Upload();
            }else{
                createSwfUpload();
            }
        }

        function createSwfUpload(){
            var sizeLimit = 5,
                commObj = {
                    file_post_name : 'Filedata',
                    upload_url : '../ajax/upimg_h.jsp?siteId=' + Fai.top.siteId,
                    button_placeholder_id : 'upload',
                    button_action : SWFUpload.BUTTON_ACTION.SELECT_FILE,
                    file_queue_limit : 1,
                    post_params : {
                        'ctrl' : 'Filedata', 'maxWidth' : 1000, 'maxHeight' : 1000,
                        'imgMode' : '2'
                    },
                    // Button setting
                    button_image_type : 2,
                    // File setting
                    file_types : '*.jpg;*.jpeg;*.bmp;*.png;*.gif',
                    file_size_limit : sizeLimit + 'MB',
                    file_dialog_complete_handler : function(){ // file_queued_handler
                    },
                    file_queue_error_handler : function( file, errCode, message ){
                        switch(errCode){
                            case SWFUpload.QUEUE_ERROR.FILE_EXCEEDS_SIZE_LIMIT :
                                Fai.ing( '图片超过' + sizeLimit + 'MB', false );
                                break;
                            case SWFUpload.QUEUE_ERROR.INVALID_FILETYPE :
                                Fai.ing( '不允许的文件类型', false );
                                break;
                            default:
                                Fai.ing( '请重新选择图片上传。', false );
                                break;
                        }
                    },
                    upload_success_handler : function(file,result){
                        result = JSON.parse(result);
                        Fai.top.$("#ing").find(".tips").remove();
                        $(".headImg").css("background-image", "url(" + result.url + ")");
                        //修改列表中的头像
                        var arrxb = 0;
                        for(var i = 0; i < Fai.top.companyNameData.length; i++){
                            if(Fai.top.companyNameData[i].siteId == siteId){
                                arrxb = i;
                                break;
                            }
                        }
                        Fai.top.companyNameData[arrxb].headerUrl = result.url;
                        Fai.ing("上传成功", true);
                        if (!Fai.top._oem){
                            window.top.$("#jz_logopopup_shadow").remove();
                        }
                    },
                    upload_error_handler : function(file,errorcode,message){
                        Fai.ing("系统错误，请稍后再试", true);
                    },
                    upload_complete_handler : function(file){
                        if( this._uploadResult && file.filestatus == SWFUpload.FILE_STATUS.COMPLETE){

                        }else{
                            Fai.ing("系统错误，请稍后再试", true);
                        }
                    },
                    upload_start_handler : function(file){
                        Fai.ing("正在上传", false);
                    },
                    upload_progress_handler : function(file, bytesLoaded, bytesTotal){

                    }
                };
            swfObj = SWFUploadCreator.create( commObj );
        }

        function createHtml5Upload(){
            var sizeLimit = 5,
                advance_setting = {
                    fileTypeExts: '*.jpg;*.jpeg;*.bmp;*.png;*.gif',
                    uploader: '../ajax/upimg_h.jsp?siteId=' + Fai.top.siteId,
                    fileObjName: 'Filedata',
                    post_params: {
                        'ctrl' : 'Filedata', 'maxWidth' : 1000, 'maxHeight' : 1000,
                        'imgMode' : '2'
                    },
                    fileSizeLimit : sizeLimit << 10 << 10,

                    onFilterError: function(file, err){
                        var errCode = err.code;
                        switch(errCode){
                            case this.Errno.OVER_SIZE:
                                Fai.ing( '图片超过' + sizeLimit + 'MB', false );
                                break;
                            case this.Errno.TYPE_ERROR :
                                Fai.ing('不允许的文件类型', false);
                                break;
                            default:
                                Fai.ing( '请重新选择图片上传。', false );
                        }
                    },
                    onUploadStart: function(file){
                        Fai.ing("正在上传", false);
                    },
                    onUploadProgress: function(file, loaded, total){

                    },
                    onUploadComplete: function(file, result){
                        result = JSON.parse(result);
                        if(result.success == true ){
                            Fai.top.$("#ing").find(".tips").remove();
                            $(".headImg").css("background-image", "url(" + result.url + ")");
                            //修改列表中的头像
                            var arrxb = 0;
                            for(var i = 0; i < Fai.top.companyNameData.length; i++){
                                if(Fai.top.companyNameData[i].siteId == siteId){
                                    arrxb = i;
                                    break;
                                }
                            }
                            Fai.top.companyNameData[arrxb].headerUrl = result.url;
                            Fai.ing("上传成功", true);
                            if (!Fai.top._oem){
                                window.top.$("#jz_logopopup_shadow").remove();
                            }
                        }
                    },
                    onUploadError: function(file, responseText){
                        Fai.top.$("#ing").find(".tips").remove();
                        Fai.ing("系统错误，请稍后再试", true);
                    }
                };
            if (Fai.top._oem){
                var upload = $('#upload-logo').uploadify(advance_setting);
            }else {
                var upload = window.top.$('#logopopup_localup').uploadify(advance_setting);
            }
        }
    });

    $(function(){
        //页面监控
        var jsMonitor;
        $.ajax({
            url: '/ajax/log_h.jsp?cmd=monitorManageMode',
            type: 'get',
            success: function(result) {
                var res = $.parseJSON(result);
                if ((typeof res.jsData != undefined) && (res.jsData != '')) {
                    jsMonitor = res.jsData;
                    $("#allSite").append(jsMonitor);
                }
            }
        });
    })
</script>
