<style type="text/css">
    .wxQrCodeExtPanel{position:fixed;left:30px;bottom:30px;z-index: 999;}
    .wxQrCodePanelInner{position: relative;}
    .fkWXQrExCode{width:100px;height:100px;margin:22px auto 0 auto;background: #fff url("//jzas.faisys.com/image/fkscQr.jpg") center center no-repeat;background-size:100% 100%;text-align: center;line-height: 111px;}
    .fkWXQrCode{width:100px;height:100px;margin:22px auto 0 auto;background: #fff url("//jzas.faisys.com/image/fkscQr.jpg") center center no-repeat;background-size:100% 100%;text-align: center;line-height: 125px;}
