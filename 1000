<script type="text/javascript">
	var httpsCertWillExpire = function(domain){
		if( false){
			var confirmContent = new Array();
			confirmContent.push( "<div style='width: 80px; height:80px; border-radius:50%; border: 4px solid gray; border: 4px solid gray; margin: 20px auto; padding: 0; position: relative; top:25px; box-sizing: content-box; border-color: #F8BB86;'>" );
			confirmContent.push(	"<div style='animation: pulseWarningIns 0.75s infinite alternate; -webkit-animation: pulseWarningIns 0.75s infinite alternate;'>");
			confirmContent.push( 		"<span style='position: absolute; width: 5px; height: 47px; left: 50%; top: 10px; webkit-border-radius: 2px; border-radius: 2px; margin-left: -2px; background-color: #F8BB86;'></span>" );
			confirmContent.push( 		"<span style='position: absolute; width: 7px; height: 7px; -webkit-border-radius: 50%; border-radius: 50%; margin-left: -3px; left: 50%; bottom: 10px; background-color: #F8BB86;'></span>" );
			confirmContent.push(	"</div>");
			confirmContent.push( "</div>" );
			confirmContent.push( "<div style='color:rgb(51,51,51); font-family: 微软雅黑; font-size:16px; padding-top: 48px; text-align:left;margin:auto 50px;'>您的域名"+domain+"的https证书即将过期 , 过期后https安全功能将失效 , 请及时前往证书平台续费。");
			confirmContent.push( "</div>");
			confirmContent.push( "<div style='text-align: center; margin-bottom: 30px; margin-top: 65px;'> " );
			confirmContent.push( 	"<input id='seeDetails' style='width: 120px; height: 40px; font-size: 13px; border: 1px solid #557ce1; background: #557ce1; font-family: 微软雅黑; color: #fff; cursor: pointer;' type='button' value='查看详情'/>" );
			confirmContent.push( "</div>" );
			var options = {
				boxId: 111,
				htmlContent: confirmContent.join(""),
				width: 568,
				height: 360,
				extClass: "fk-ssl-remind",
				popupBoxZIndex: 9999,
				closeFunc:function(){
					$.ajax({
						type:"POST",
						url:"../ajax/jzallsite_h.jsp?cmd=setSSL_Flag",
						data:"sslCertId=-1"
					});
				}
			}
			//whale
			/*var sslCertReminds = Fai.top.localStorage.sslCertReminds;
			if(!sslCertReminds){
				sslCertReminds="";
				Fai.top.localStorage.sslCertReminds = sslCertReminds;
			}
			var domains = sslCertReminds.split("#");
			$.each(domains,function(){
				
			});*/
			var popupWin = Site.popupBox(options);
			popupWin.on("click","#seeDetails",function(){
				if (false) { // index.jsp定义了isNewSiteStat
					if (window.parent && parent.actions) {
						if (false) {
							parent.actions.openSubMenu("manageFrameBannerSiteDomain","","", "manageFrameMobileSiteDomain");
						} else {
							parent.actions.openSubMenu("manageFrameBannerSiteDomain","","", "manageFrameSiteDomain");
						}
						popupWin.find(".popupBClose").click();
					}
					return;
				}

				popupWin.find(".popupBClose").click();
				$(".J_manage").click(); //进入网站管理
				$("#topManageFrame").load(function(){
					$("#topManageFrame").contents().find(".jz_scrollbar__view .menu_item .icon_setting").click(); //进入系统设置
					$("#topManageFrame").contents().find(".jz_scrollbar__view .menu_item .icon_setting + ul li").each(function(){
						//进入网站域名
						var text = '网站域名';
						if($(this).text().indexOf(text) != -1){
							$(this).click();
							return;
						}
					}); 
					$("#topManageFrame").contents().find("#manageFramePage").load(function(){
						$("#topManageFrame").contents().find("#manageFramePage").contents().find("#listForm li").each(function(){ 
							if($(this).text().indexOf(domain) != -1){
								$(this).parent('ul').find('a[id^=httpsSet]').click();
								return;
							} 
						});
					});
				});
			});
		}
	}
	$(function(){
		httpsCertWillExpire("vb24076366.mallfkw.cn"); //弹框提醒
	})
</script>
<div  class="manage_layout" v-if="!isOem">
    <!-- 头部栏 start -->
    <div id="manageTopBar" class="manage_layout_topBar" v-cloak>
        <div class="mall_top_bar">
            <div class="mall_logo" v-if="!isOem" @click="openHomeHost">
                <svg class="svg_link" width="138" height="35"><use xlink:href="#icon_mall_logo"></use></svg>
            </div>
            <div class="mall_logo oem_mall_logo" v-else>
                <svg class="svg_link" width="138" height="30"><use xlink:href="#icon_trademark02"></use></svg>
            </div>
            <template v-if="isJZAllSiteStatPage && !isOem">
                <div class="mallTips" id="mallTips" :class="isOem ? 'fk-toolTopBarV2': ''">
                    <div id='toolbarLeft' class='toolbarLeftV2'>
                        
                        <fk-10year-promotion  :itemdata='fk10YearPromotion' v-if='fk10YearPromotion.isShow && !isOem' :key='fk10YearPromotion.type'></fk-10year-promotion>
                        <buyer-question-guide :itemdata='mallBuyerQuestion' v-if='mallBuyerQuestion.isShow' :key='mallBuyerQuestion.type'></buyer-question-guide>
                        <fk-expires-date  :itemdata='fkExpiresDatePanel' v-if='fkExpiresDatePanel.isShow && !isOem' :key='fkExpiresDatePanel.type'></fk-expires-date>
                        <fk-admtool-eight-years  :itemdata='fkEightYearsPanel' v-if='fkEightYearsPanel.isShow' :key='fkEightYearsPanel.type'></fk-admtool-eight-years>
                    </div>
                    <div id='toolbarRight' class='toolbarRightV2' :class="isOem ? 'toolbarRight': ''">
                        <fk-admtool-new-vip  :itemdata='fkNewVipPanel' v-if='fkNewVipPanel.isShow' :key='fkNewVipPanel.type'></fk-admtool-new-vip>
                        
                        

                        
                        
                        

                        <div  class='toolbarItem '>
                            <button-component class='riseBtn riseBtnV2'  v-if='(topVerBySiteId < 45 ) || (topVerBySiteId == 110)'  @click='updateVersion(siteId)'>购买升级</button-component>
                            <fk-coupon :itemdata='coupon' v-if="!isOem"></fk-coupon>
                            <fk-suggest :itemdata='fkSuggestItem' v-if="!isOem"></fk-suggest>
